<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="EditTeset">	
	
	<resultMap id="getBoardListResult" class="HashMap">
		<result property="EXAM_MNG_NO" column="EXAM_MNG_NO" />
		<result property="EXAM_OUTLINE" column="EXAM_OUTLINE" jdbcType="CLOB" javaType="java.lang.String" />
	</resultMap>
    
    <insert id="insertEdit" parameterClass="HashMap">
    	<selectKey keyProperty="id" resultClass="int">
    	   SELECT MAX(EXAM_MNG_NO) + 1 AS ID FROM RDEI101MT
	   </selectKey>

    	INSERT INTO RDEI101MT (EXAM_MNG_NO, EXAM_OUTLINE) VALUES (#id#, #content1#)
    </insert>
    
    <select id="selectEdit" parameterClass="HashMap"  resultMap="getBoardListResult">
        /* TestAnnounce.selectEdit */
        SELECT EXAM_MNG_NO, EXAM_OUTLINE FROM RDEI101MT WHERE EXAM_MNG_NO = (SELECT MAX(EXAM_MNG_NO)  AS ID FROM RDEI101MT WHERE EXAM_OUTLINE is not null)        
    </select>      

</sqlMap>
