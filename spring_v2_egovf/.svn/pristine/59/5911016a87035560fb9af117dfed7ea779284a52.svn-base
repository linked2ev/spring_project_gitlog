<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>

<jsp:include page="/WEB-INF/jsp/fo/include/head.jsp" />
</head>
<body>
<form id="form" name="form" method="post">
	<input type="hidden" id="returnUrl" name="returnUrl" value="<c:out value="${loginReturnUrl}"/>"/>
	<input type="hidden" id="<c:out value="${returnKey}"/>" name="<c:out value="${returnKey}"/>" value="<c:out value="${returnVal}"/>"/>
	<input type="hidden" id="<c:out value="${returnKey1}"/>" name="<c:out value="${returnKey1}"/>" value="<c:out value="${returnVal1}"/>"/>
	<input type="hidden" id="<c:out value="${returnKey2}"/>" name="<c:out value="${returnKey2}"/>" value="<c:out value="${returnVal2}"/>"/>
</form>
<c:choose>
	<c:when test="${!empty alertMsg}">
		<script type="text/javascript">
		    $(document).ready(function () {
		    	
		    	var locationYN = '<c:out value="${location}"/>';  

		    	returnMsg.alertMsg("<c:out value='${alertMsg}' escapeXml='false'/>", "returnUrl", function(){ $("#fn_closeAlert").trigger("click"); });
		    	 
		     	$(document).on("click", "#fn_closeAlert", function(){
		     		
		     		if("Y" === locationYN){
		     			location.href = '<c:out value="${returnUrl}"/>';
		     		}else{
		    			$("#form").attr({"action":"<c:url value='${returnUrl}'/>", "target":"_self", "method":"post"}).submit();
		     		}
		    	});
		    });
		</script>
	</c:when>
	<c:otherwise>
		<c:choose>
			<c:when test="${!empty confirmMsg}">
				<script type="text/javascript">
				    $(document).ready(function () {
				    	returnMsg.confirmMsg('<c:out value="${confirmMsg}"/>', "confirm", function(){ fn_memberRecovery(); });
				    	
				    	function fn_memberRecovery(){
				    		$("#form").attr({"action":"<c:url value='${returnUrl}'/>", "target":"_self", "method":"post"}).submit();
				    	}
				    });
				</script> 
			</c:when>
			<c:otherwise>
				<script type="text/javascript">
				    $(document).ready(function () {
				    	
				    	var returnKey = '<c:out value="${returnKey}"/>';  
				    	
				    	if(returnKey == null || returnKey == ""){
				    		location.href = '<c:out value="${returnUrl}"/>';
				    	}
				    	else{
				    		$("#form").attr({"action":"<c:url value='${returnUrl}'/>", "target":"_self", "method":"post"}).submit();
				    	}
				    });
				</script>
			</c:otherwise>
		</c:choose>
	</c:otherwise>
</c:choose>
</body>

</html>