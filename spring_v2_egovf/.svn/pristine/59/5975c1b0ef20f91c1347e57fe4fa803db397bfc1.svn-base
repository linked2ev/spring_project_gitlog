function setGridHeader(jqGridId){
	var myId = "#"+jqGridId;
	
	//테이블과 헤더의 width를 잡아준후 jqgfirstrow 숨김
	var item = $(myId + " .jqgfirstrow td");

	var str = "<colgroup>";
	
	for(var i = 0;i < item.length;i++){			
		var $header = $("#gbox_"+jqGridId + " .ui-jqgrid-labels > th:eq("+i+")");
		$header.css("width", $(item[i]).width()+"px");
		
		//hidden으로 셋팅된 column은 숨긴다
		if($header.css("display") == "none"){
			str += "<col style='width:" + $(item[i]).width() + "px; display:none;'>";
		}else{
			str += "<col style='width:" + $(item[i]).width() + "px;'>";
		}
	}
	str += "</colgroup>";
	// [수정 전] colgroup 추가 후 jqgfirstrow는 숨김
	//$(myId + " .jqgfirstrow").parent().parent().append(str);
	//$(myId + " .jqgfirstrow").hide();
	 
	//colgroup가 이미 있다면 삭제후 추가 
	var appendElement = $(myId + " .jqgfirstrow").parent().parent();
	
	if(appendElement.has("colgroup").length > 0){
		appendElement.children("colgroup").remove(); 
	}
	appendElement.append(str);
	$(myId + " .jqgfirstrow").hide();  
  
	//헤더에 panel_style.css에 있는 클래스 추가
	$(myId + " .ui-jqgrid-hbox").children().addClass("tbl-in-tbl visit-grid01");
}

/**
 * 작성자: Dodam
 * 폼안에 값을 넘길 Input Hidden을 생성
 * @param jqGridId
 * @returns 
 */
function createHiddenInput(form, name, value){
	var el = document.createElement("input");
	el.type = "hidden";
	el.className = "HiddenInput";
    el.name = name;
    el.value = value;
    $(form).append(el);  
    
    return el;
}

/**
 * 작성자: Dodam
 * 서베이 기본 셋팅
 * @param jqGridId
 * @returns 
 */
function makeSurveyObj(id, surveyTxt, display, answerTxt){
	var tempSurvey; 

	if(surveyTxt != ""){
		tempSurvey = new Survey.Model('{"pages": '+surveyTxt+' }');
		tempSurvey.questionTitleTemplate = "{no}) {title}";
		tempSurvey.showPageTitles = false;
		tempSurvey.showPageNumbers = false;
		tempSurvey.showNavigationButtons = false;
		
		//답변선택 못하도록
		if(display){
			tempSurvey.mode = 'display';
		}
		
		tempSurvey.data = answerTxt;
		tempSurvey.render(id);
	}

	return tempSurvey;
}

/**
 * 작성자: Dodam
 * 서베이 답변만 셋팅
 * @param jqGridId
 * @returns 
 */
function setSurveyData(id, answerTxt, surveyObj){
	var tempSurvey = surveyObj;

	tempSurvey.data = answerTxt;
	tempSurvey.render(id);
 
	return tempSurvey;
}

/**
 * 작성자: Dodam
 * 서베이 저장 전 빈값 체크
 * @param editor.getAllQuestions()
 * @returns 
 */
var getEditor, surveyQuestionId, pageNum;
function surveyRequiredCheck(editor){	
	getEditor = editor;
	var allItems = getEditor.getAllQuestions();

	//질문이 없을때
	if(!findPageQuestions(getEditor)){
		returnMsg.alertMsg("저장할 질문이 없습니다.");
  
		return false;
	}

	var pageItems = editor.getSurveyJSON();
	var questionLength = 0;
	
	//각 페이지 별 질문의 갯수를 합치고 패널 체크
	for(var i = 0;i < pageItems.pages.length;i++){
		var pageArr = pageItems.pages[i].elements;
		
		if(pageArr != null){
			for(var j = 0;j < pageArr.length;j++){
				//하위 요소가 있다면(패널)
				if(pageArr[j].elements != null){
					//패널 이름이 초기에 지정된 이름이라면
					if(pageArr[j].title == null){
						pageNum = i;
						
						returnMsg.alertMsg("타이틀을 입력해주세요.", "", function(){
							$(".svd_page_nav:eq('"+pageNum+"')").children().trigger("click");
							$("#sq_page div span:contains('"+pageArr[j].name+"')").trigger("click");
						});
						return false;
					}
					//패널 안에 하위 질문들을 더한다
					else{
						questionLength += pageArr[j].elements.length;
					}
				}
				//하위 요소가 없다면
				else{
					//패널 타입이라면
					if(pageArr[j].type == "panel"){
						pageNum = i;
						
						returnMsg.alertMsg("패널 안에 질문을 넣어주세요.", "", function(){
							$(".svd_page_nav:eq('"+pageNum+"')").children().trigger("click");
							$("#sq_page div span:contains('"+pageArr[j].name+"')").trigger("click");
						});	
						return false;
					}
					//해당 질문을 더한다
					else{
						questionLength += 1;
					}
				}
			}
		}else{
			pageNum = i;
			
			returnMsg.alertMsg("빈 페이지가 있습니다.", "", function(){
				$(".svd_page_nav:eq('"+pageNum+"')").children().trigger("click");
			});	
			
			return false;
		}
	}
	
	//총 질문의 갯수만큼 for문을 돈다
	for(var i = 0;i < questionLength;i++){
		//타이틀
		if(allItems[i].name == allItems[i].title){
			surveyQuestionId = allItems[i].id;
			
			returnMsg.alertMsg("타이틀을 입력해주세요", "", function(){
				$(".svd_page_nav:eq('"+findPage(surveyQuestionId)+"')").children().trigger("click");
				$("#"+surveyQuestionId + " .image_top").trigger("click");
			});	
			
			return false;
		}
		
		//열
		if(allItems[i].columnsValue != null){
			for(var j = 0;j < allItems[i].columnsValue.length;j++){
				//일반
				if(allItems[i].columnsValue[j].hasText != null){
					if(!allItems[i].columnsValue[j].hasText){
						surveyQuestionId = allItems[i].id;
						
						returnMsg.alertMsg("열에 미입력사항이 있습니다", "", function(){
							$(".svd_page_nav:eq('"+findPage(surveyQuestionId)+"')").children().trigger("click");
							$("#"+surveyQuestionId + " .image_top").trigger("click");
							$("ul.nav-tabs").children("li:contains('열')").trigger("click");
						});	
						
						return false;
					}
				}
				//matrix(multiple)
				else{
					if(allItems[i].columnsValue[j].name == allItems[i].columnsValue[j].title){
						surveyQuestionId = allItems[i].id;
						
						returnMsg.alertMsg("열에 미입력사항이 있습니다", "", function(){						
							$(".svd_page_nav:eq('"+findPage(surveyQuestionId)+"')").children().trigger("click");
							$("#"+surveyQuestionId + " .image_top").trigger("click");
							$("ul.nav-tabs").children("li:contains('열')").trigger("click");
						});	
						
						return false;
					}
				}
			}
		}
		//열
		if(allItems[i].rowsValue != null){
			for(var j = 0;j < allItems[i].rowsValue.length;j++){
				if(!allItems[i].rowsValue[j].hasText){
					surveyQuestionId = allItems[i].id;
					
					returnMsg.alertMsg("행에 미입력사항이 있습니다", "", function(){
						$(".svd_page_nav:eq('"+findPage(surveyQuestionId)+"')").children().trigger("click");
						$("#"+surveyQuestionId + " .image_top").trigger("click");
						$("ul.nav-tabs").children("li:contains('행')").trigger("click");
					});	
					
					return false;
				}
			}
		}
		//보기
		if(allItems[i].choices != null){
			for(var j = 0;j < allItems[i].choices.length;j++){
				if(!allItems[i].choices[j].hasText){
					surveyQuestionId = allItems[i].id;
					
					returnMsg.alertMsg("보기에 미입력사항이 있습니다", "", function(){					
						$(".svd_page_nav:eq('"+findPage(surveyQuestionId)+"')").children().trigger("click");
						$("#"+surveyQuestionId + " .image_top").trigger("click");
						$("ul.nav-tabs").children("li:contains('보기')").trigger("click");
					});	
					
					return false;
				}
			}
		}
	}
	return true;
}

/**
 * 작성자: Dodam
 * 서베이 질문 타입 제한
 * @param questionId
 * @returns 
 */
function surveyQuestionTypeCheck(editor){	
	getEditor = editor;
	var allItems = getEditor.getAllQuestions();

	var pageItems = editor.getSurveyJSON();
	var questionLength = 0;
	
	//각 페이지 별 질문의 갯수를 합치고 패널 체크
	for(var i = 0;i < pageItems.pages.length;i++){
		var pageArr = pageItems.pages[i].elements;
		
		for(var j = 0;j < pageArr.length;j++){
			//하위 요소가 있다면(패널)
			if(pageArr[j].elements != null){
				//패널 안에 하위 질문들을 더한다
				if(pageArr[j].title != null){
					questionLength += pageArr[j].elements.length;
				}
			}
			//하위 요소가 없다면
			else{
				//패널 타입이 아니라면
				if(pageArr[j].type != "panel"){
					questionLength += 1;
				}
			}
		}		
	}
	
	//총 질문의 갯수만큼 for문을 돈다
	for(var i = 0;i < questionLength;i++){
		//타이틀
		if(allItems[i].getType() != "radiogroup" && allItems[i].getType() != "matrix"){
			surveyQuestionId = allItems[i].id;
			
			returnMsg.alertMsg("사용할 수 없는 질문 타입입니다", "", function(){
				$(".svd_page_nav:eq('"+findPage(surveyQuestionId)+"')").children().trigger("click");
				$("#"+surveyQuestionId + " .image_top").trigger("click");
			});	
			
			return false;
		}
	}
	return true;
}

/**
 * 작성자: Dodam
 * 서베이 현재 질문의 페이지 찾기
 * @param questionId
 * @returns 
 */
function findPage(questionId){
	var page = getEditor.survey.pages;
	
	if(page.length == 0){
		return 1;
	}else{
		for(var i = 0;i < page.length;i++){
			var pageQuestions = page[i].questions;
			
			for(var j = 0;j < pageQuestions.length;j++){
				if(pageQuestions[j].id == questionId){
					return i;
				}
			}
		}
	}
}

/**
 * 작성자: Dodam
 * 각 페이지의 질문 여부
 * @param questionId
 * @returns 
 */
function findPageQuestions(editor){
	var pageItems = editor.getSurveyJSON();
	var result = false;
	
	for(var i = 0;i < pageItems.pages.length;i++){
		if(pageItems.pages[i].elements != null){
			result = true;
		}
	}
	return result;
}

/**
 * 작성자: Dodam
 * JSON.parse시 빈값은 그냥 리턴
 * @param questionId
 * @returns 
 */
function jsonParseReturn(str){
	if(str == ""){
		return "";
	}else{
		return JSON.parse(str);
	}
}

/**
 * 작성자: jamuge
 * 특정 div 특정 영역 인쇄
 * @returns {Boolean}
 */
function printDivHtml(){
	
	var initBody = document.body.innerHTML;
	window.onbeforeprint = function () {
		document.body.innerHTML = "<div style='font-size:25px; text-align:center;'>" + document.getElementById("printTitleDiv").innerHTML + "</div></br>" + document.getElementById("printDiv").innerHTML;
	};
	window.onafterprint = function () {
		document.body.innerHTML = initBody;
	};
	
	//$(".ui-jqgrid-bdiv").css('height','auto'); 
	//$(".tbl-scroll").removeClass();
	var obj = "<div style='font-size:25px; text-align:center;'>" + document.getElementById("printTitleDiv").innerHTML + "</div></br>" + document.getElementById("printDiv").innerHTML;

	windowObj = window.open('','','width=800,height=800');
	windowObj.document.write(obj);

	windowObj.document.close();
	windowObj.focus();
	windowObj.print(); 
}