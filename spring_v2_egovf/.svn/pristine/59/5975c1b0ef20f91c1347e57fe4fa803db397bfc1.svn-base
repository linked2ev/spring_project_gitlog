//창 이동/닫히기 전에 발생하는 이벤트
window.onbeforeunload = function(e){
    if(e != null && e != undefined){
        jQuery('#Load').css('display','');
    }
};

window.onload = function(e){
	$('#Load').css('display', 'none');
};

