<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%><jsp:include page="/WEB-INF/jsp/fo/include/headGrid.jsp" /></head><body><!-- header --><jsp:include page="/WEB-INF/jsp/fo/include/header/headerPanel${panelAuth }.jsp" flush="false" /><!-- // header --><!-- main --><main id="main" class="main respWidth">	<div class="containerWrap">		<!-- leftNav -->  	<jsp:include page="/WEB-INF/jsp/fo/include/left/leftPanel${panelAuth }.jsp" flush="false" />	<!-- // leftNav --> 			<div class="pageWrap rndModiInfo">			<div class="page-title">				<h2><spring:message code="menu.006"/></h2>				<div class="page-location">					<span class="home"><spring:message code="menu.010"/></span> - <spring:message code="menu.003"/> - <b><spring:message code="menu.006"/></b>				</div>			</div>						<div class="page-content">				<div class="tblWrap tbl-wm">					<form id="form" name="form">										<table>						<caption><spring:message code="member.000"/></caption>						<colgroup>							<col width="22%">							<col width="78%">						</colgroup>						<tbody>							<tr>								<th><spring:message code="member.001"/></th>								<td><c:out value="${info.NAME }"/></td>							</tr>							<tr>								<th><spring:message code="member.002"/></th>								<td>									<ul class="divForm phoneNo">										<li>											<select id="PHONE1" name="PHONE1" class="inputBox requireFileld" title="<spring:message code="member.003"/>">												<c:if test="${not empty PHONE1_CD_LIST }">			                                        <c:forEach var="codeRow" items="${PHONE1_CD_LIST }" varStatus="i">			                                            <option value="<c:out value="${codeRow.VALUE }"/>" <c:if test="${codeRow.VALUE eq info.PHONE1 }">selected="selected"</c:if>>			                                                <c:out value="${codeRow.VALUE }"/>			                                            </option>			                                        </c:forEach>			                                    </c:if>											</select>										</li>										<li class="divLetter"><span>-</span></li>										<li><input type="tel" id="PHONE2" name="PHONE2" class="inputBox fnNumber requireFileld" maxlength="4" title="<spring:message code="member.004"/>" value="<c:out value="${info.PHONE2}"/>" ></li>										<li class="divLetter"><span>-</span></li>										<li><input type="tel" id="PHONE3" name="PHONE3" class="inputBox fnNumber requireFileld" maxlength="4" title="<spring:message code="member.005"/>" value="<c:out value="${info.PHONE3}"/>" ></li>									</ul>								</td>							</tr>							<tr>								<th><spring:message code="member.006"/></th>								<td>									<div class="divCheck">										<input type="radio" id="infoMale" name="GENDER_CD" <c:if test="${info.GENDER_CD eq 'M' }">checked="checked"</c:if> value="M" >										<label for="infoMale"><spring:message code="member.007"/></label>										<input type="radio" id="infoFemale" name="GENDER_CD" <c:if test="${info.GENDER_CD eq 'W' }">checked="checked"</c:if> value="W" >										<label for="infoFemale"><spring:message code="member.008"/></label>									</div>								</td>							</tr>							<tr>								<th><spring:message code="member.009"/></th>								<td>									<ul class="divForm birthDay">										<li> 											<select id="YEAR" name="YEAR" class="inputBox">  												<c:set var="today" value="<%=new java.util.Date()%>" />										        <fmt:formatDate value="${today}" pattern="yyyy" var="start"/> 										            <c:forEach begin="10" end="100" var="idx" step="1">											            <option value="<c:out value="${start -idx}" />" <c:if test="${start -idx eq info.YEAR }">selected="selected"</c:if> > 															<c:out value="${start -idx}" />														</option> 										          	</c:forEach>											</select>										</li>										<li class="divLetter"><span><spring:message code="member.010"/></span></li>										<li>											<select id="MONTH" name="MONTH" class="inputBox">									            <c:forEach begin="1" end="12" var="idx" step="1"> 										            <option value="<c:out value="${begin + idx}" />" <c:if test="${begin + idx eq info.MONTH }">selected="selected"</c:if> > 														<c:out value="${begin + idx}" />													</option>									          	</c:forEach>											</select>		 										</li> 										<li class="divLetter"><span><spring:message code="member.011"/></span></li>										<li>											<select id="DAY" name="DAY" class="inputBox">												<c:forEach begin="1" end="31" var="idx" step="1"> 										            <option value="<c:out value="${begin + idx}" />" <c:if test="${begin + idx eq info.DAY }">selected="selected"</c:if> > 														<c:out value="${begin + idx}" />													</option>									          	</c:forEach>											</select>																		</li>										<li class="divLetter"><span><spring:message code="member.012"/></span></li>									</ul>								</td>							</tr>							<tr>								<th><spring:message code="member.013"/></th>								<td>									<div class="divCheck">										<c:forEach var="codeRow" items="${OCCU_AREA_LIST }" varStatus="i">                                            <input type="radio" id="area<c:out value="${codeRow.CODE }"/>" name="OCCU_AREA_CD" <c:if test="${info.OCCU_AREA_CD eq codeRow.CODE }">checked="checked"</c:if> value="<c:out value="${codeRow.CODE }"/>">											<label for="area<c:out value="${codeRow.CODE }"/>"><c:out value="${codeRow.VALUE }"/></label>                                        </c:forEach>									</div> 								</td>							</tr>							<tr>								<th><spring:message code="member.014"/></th>								<td>									<div class="divCheck">										<c:forEach var="codeRow" items="${VISIT_PREFER_LIST }" varStatus="i">                                            <input type="radio" id="visit<c:out value="${codeRow.CODE }"/>" name="VISIT_PREFER_CD" <c:if test="${info.VISIT_PREFER_CD eq codeRow.CODE }">checked="checked"</c:if> value="<c:out value="${codeRow.CODE }"/>">											<label for="visit<c:out value="${codeRow.CODE }"/>"><c:out value="${codeRow.VALUE }" /></label>                                        </c:forEach>									</div>								</td>							</tr>							<tr>								<th><spring:message code="member.015"/></th>								<td>									<ul class="divForm panelSet">										<li>											<input type="radio" id="BREAK_N" name="BREAK_YN" <c:if test="${info.BREAK_YN ne 'Y'}">checked="checked"</c:if> value="N">											<label for="BREAK_N"><spring:message code="member.016"/></label>										</li>										<li> 											<input type="radio" id="BREAK_Y" name="BREAK_YN" <c:if test="${info.BREAK_YN eq 'Y'}">checked="checked"</c:if> value="Y">											<label for="BREAK_Y"><spring:message code="member.017"/></label>											<br class="br-mobile">											<div class="inputBoxDate" style="width:140px;">												<input type="text" id="_BREAK_FROM" name="_BREAK_FROM" class="inputBox datepicker s-datepicker" value="<c:out value="${info.BREAK_FROM }"/>" <c:if test="${info.BREAK_YN eq 'N'}">disabled="true"</c:if>>												<input type="hidden" id="BREAK_FROM" name="BREAK_FROM" class="inputBox">											</div>											&nbsp;~&nbsp;											<div class="inputBoxDate" style="width:140px;">												<input type="text" id="_BREAK_TO" name="_BREAK_TO" class="inputBox datepicker e-datepicker" value="<c:out value="${info.BREAK_TO }"/>" <c:if test="${info.BREAK_YN eq 'N'}">disabled="true"</c:if>>												<input type="hidden" id="BREAK_TO" name="BREAK_TO" class="inputBox">											</div>										</li>										<li>											<span class="txtRed"><spring:message code="myInfo.000"/></span>										</li>									</ul>								</td>							</tr>						</tbody>					</table>					</form>				</div>				<div class="btn-wrap btnC">					<button type="button" class="btn btn-prim btn-red wd100" id="fn_update"><spring:message code="button.001"/></button>				</div>			</div><!-- // page-content -->		</div><!-- // pageWrap -->	</div><!-- // container --></main><!-- // main --><!-- footer --><jsp:include page="/WEB-INF/jsp/fo/include/footer.jsp" /><!-- // footer --><script>$(function(){		var breakYn = "${info.BREAK_YN}";	if(breakYn != "Y"){		$("#_BREAK_FROM").val("");		$("#_BREAK_TO").val("");	}	// 패널 비활동 기간 항목 라디오 이벤트	$(document).on("change", "input[name='BREAK_YN']", function(){		$("#_BREAK_FROM").val("");		$("#_BREAK_TO").val("");				if(this.value == "N"){			$("#_BREAK_FROM").attr("disabled", true);			$("#_BREAK_TO").attr("disabled", true); 		}else{			$("#_BREAK_FROM").attr("disabled", false);			$("#_BREAK_TO").attr("disabled", false);		}	});		// 수정 버튼 클릭 이벤트	$(document).on("click", "#fn_update", function(){		var $form = $("form");				// 패널 비활동 기간 사용 시		dpVal = $("input[name=BREAK_YN]:radio:checked").val();				// FORM 필수값 검사		if(isEmptyForm("form")){			$("#BREAK_FROM").val($("#_BREAK_FROM").val().replace(/-/g, ""));			$("#BREAK_TO").val($("#_BREAK_TO").val().replace(/-/g, ""));						$form.attr({"action":"<c:url value='/fo/mp/myInfoUpdate.do'/>", "target":"_self", "method":"post"}).submit();		}	});});</script></body></html>