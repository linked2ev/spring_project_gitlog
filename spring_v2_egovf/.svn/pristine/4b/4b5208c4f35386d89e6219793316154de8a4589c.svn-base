ng/summernote-ko-KR.js"></script> <!-- <link rel="stylesheet" href="../../common/css/panel_style.css" type="text/css"> -->	<script type="text/javaScript" language="javascript" defer="defer">	$(document).ready(function() {		$('.summernote').summernote({						});		     			});	    	         		$(function(){		// 저장 버튼 클릭 이벤트	    $(document).on("click", "#insertEdit", function () {	        var $frm = $("#frm");	        // 	        $frm.attr({"action":"<c:url value='/fo/ta/insertEdit.do'/>", "target":"_self", "method":"post"}).submit();	        	        $.ajax({	            async : false,	            type : "POST",	            dataType: "json",	            data : $frm.serialize(),	            url : "<c:url value='/fo/ta/insertEdit.do'/>",	            success : function (data){	            		            	if(data.result != null){	            		            			            		            		alert("정상적으로 등록되었습니다.");	            			            	}else{	            		alert("NULL !!!!!");		            	}	            },	            error : function (err){	            	alert("등록중에 시스템 오류가 발생하였습니다.\n[" + err.status + "]");	            }	        });	    });			 	// 조회 버튼 클릭 이벤트	    $(document).on("click", "#selectEdit", function () {	        	        $.ajax({	            async : false,	            type : "POST",	            dataType: "json",	            data : "",	            url : "<c:url value='/fo/ta/selectEdit.do'/>",	            success : function (data){	            		            	if(data != null){	            		            			            		            		$('#summernote2').summernote('code', data.result.EXAM_OUTLINE);	            		$("#resultEdit").append(data.result.EXAM_OUTLINE);	            			            	}else{	            		alert("NULL !!!!!");		            	}	            },	            error : function (err){	            	alert("등록중에 시스템 오류가 발생하였습니다.\n[" + err.status + "]");	            }	        });        	    });	});</script></head><body><form id="frm" name="frm"><div style="width: 1200px; margin: auto;"><textarea name="content1" id="summernote1" value="" class="summernote"></textarea></div><!-- <div style="width: 1200px; margin: auto;"> --><!-- <textarea name="content2" id="summernote2" value="" class="summernote"></textarea> --><!-- </div> --><input type="button" class="btn btn-search" id="insertEdit" value="저장"><i class="ico-search"></i></input><input type="button" class="btn btn-search" id="selectEdit" value="조회"><i class="ico-search"></i></input><div id="resultEdit"></div>	</form></body></html>