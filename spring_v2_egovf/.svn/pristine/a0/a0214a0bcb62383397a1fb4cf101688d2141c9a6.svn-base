@charset "utf-8";

@font-face {
	font-family:"arDotum";
	src:url('../fonts/Arita-Dotum-Medium.eot');
	src:url('../fonts/Arita-Dotum-Medium.eot?#iefix') format('embedded-opentype'),
	url('../fonts/Arita-Dotum-Medium.woff') format('woff'),
	url('../fonts/Arita-Dotum-Medium.ttf') format('truetype');
	src:local(※), url('../fonts/Arita-Dotum-Medium.woff') format('woff');
    font-weight:400;
    font-style:normal;
}
@font-face {
	font-family:"arDotum";
	src:url('../fonts/Arita-Dotum-SemiBold.eot');
	src:url('../fonts/Arita-Dotum-SemiBold.eot?#iefix') format('embedded-opentype'),
	url('../fonts/Arita-Dotum-SemiBold.woff') format('woff'),
	url('../fonts/Arita-Dotum-SemiBold.ttf') format('truetype');
	src:local(※), url('../fonts/Arita-Dotum-SemiBold.woff') format('woff');
    font-weight:500;
    font-style:normal;
}
@font-face {
	font-family:"arDotum";
	src:url('../fonts/Arita-Dotum-Bold.eot');
	src:url('../fonts/Arita-Dotum-Bold.eot?#iefix') format('embedded-opentype'),
	url('../fonts/Arita-Dotum-Bold.woff') format('woff'),
	url('../fonts/Arita-Dotum-Bold.ttf') format('truetype');
	src:local(※), url('../fonts/Arita-Dotum-Bold.woff') format('woff');
    font-weight:600;
    font-style:normal;
}
/*---------------   Reset   ---------------*/
html {margin:0;padding:0}
body {margin:0; padding:0;font-family:'arDotum','맑은 고딕',Malgun Gothic,'돋움',Dotum,AppleGothic,sans-serif;font-size:15px;line-height:1.6;color:#404040;letter-spacing:0;-webkit-text-size-adjust:none;}
div,h1,h2,h3,h4,h5,h6,ul,ol,li,dl,dt,dd,p,form,fieldset,input,table,tr,th,td,button,textarea,select {margin:0;padding:0;box-sizing:border-box;}
h1, h2, h3, h4, h5, h6{font-size:100%;font-weight:normal}
section, nav, article, aside, header, footer, main {display:block;}
img {border:0;vertical-align:top;}
ul,ol,dl,dt,dd,li {list-style:none;}
table {width:100%;border-collapse:collapse;border-spacing:0;}
th, td {font-weight: normal;}
caption {display:block;text-indent:-9999em;overflow:hidden;width:0;height:0;font-size:0;line-height:0;}
caption, legend {display:none;}
fieldset, blockquote, iframe, button {border:none;}
i, em, u, cite {font-style:normal;}
em {font-weight:normal;}
strong, b {font-weight:500;}
a {color:#404040;text-decoration:none;}
a:hover, a:active, a:visited, a:focus {color:#404040;text-decoration:none;}
.screen_out {overflow:hidden;position:absolute;top:-9999px;left:-9999px;width:1px;height:1px;font-size:1px;}

/*---------------   Layout   ---------------*/
/* skip navigation */
.skip {display:block;height:1px;width:1px;margin:0 -1px -1px 0;padding:0;overflow:hidden;font-size:0;line-height:0;}
.skip:hover, .skip:active, .skip:focus {width:100%;height:auto;margin:0;padding:5px 0;text-indent:10px;font-weight:bold;font-size:12px;color:#333;line-height:1;text-decoration:none !important;}
/* common */
body {background-color:#f2f6fa;}
.header, .main, .footer {position:relative;}
.respWidth {max-width:1100px; margin:0 auto;}
main .containerWrap {min-height:560px;border:1px solid #d3d3d3;border-top:none;background-color:#fff;position:relative;}
.pageWrap {margin:0 39px 40px 259px;padding-top:40px;}
@media all and (max-width: 767px) {
	body {font-size:13px;}  
}
/* header pc */
.header-pc {height:120px;border-bottom:3px solid #084fa6;}
.header-pc h1 {display:inline-block;}
.header-pc h1 a {display:block;width:294px;height:120px;font-size:30px;font-weight:500;color:#333;line-height:1;text-indent:100%;white-space:nowrap;overflow:hidden;background:url('../images/h1_logo.png') no-repeat left 50%;background-size:100%;}
.header-pc .util {float:right;margin:50px 10px 0 0;}
.header-pc .util span {display:inline-block;margin-left:15px;font-size:14px;color:#999;position:relative;}
.header-pc .util span:before {content:'';display:inline-block;width:1px;height:12px;background-color:#d9d6cf;position:absolute;left:-10px;top:6px;}
.header-pc .util span:first-child:before {content:none;}
.header-pc .util .user-name em {font-weight:500;}
.header-pc .util .btn-logout button {font-size:14px;color:#999;line-height:1.6;outline:none;}
/* header mobile */
.header-mobile {height:64px;}
.header-mobile h1 {display:block;width:190px;margin:0 auto;}
.header-mobile h1 a {display:block;height:64px;text-indent:100%;white-space:nowrap;overflow:hidden;background:url('../images/h1_logo.png') no-repeat center center;background-size:100%;}
.header-mobile, .mobile-menu {display:none;}
/* sidebar : mobile navi */
.sidebar-mobile {position:fixed;top:0;bottom:0;width:300px;background:#fff;}
.sidebar-mobile[data-sidebar-main="closed"] {margin-left:-50px;}
.sidebar-mobile[data-sidebar-main="closed"] {display:none !important;}
.sidebar-mobile[data-sidebar-main="opened"] {display:block !important;}
.sidebar-close {display:block;width:24px;height:24px;position:absolute;right:-44px;top:20px;background:url('../images/close_sidebar.png');background-size:100%;cursor:pointer;}
.sidebar-container {height:100%;position:relative;}
.sidebar-logo {width:100%;height:65px;border-bottom:1px solid #e0e0e0;background:#fff;position:absolute;z-index:2;}
.sidebar-logo h1 {display:block;width:190px;height:65px;margin:0 auto;font-size:30px;font-weight:500;color:#333;line-height:1;text-indent:100%;white-space:nowrap;overflow:hidden;background:url('../images/sidebar_logo.png') no-repeat center 50%;background-size:100%;}
.sidebar-nav {height:100%;min-height:100%;padding:65px 0 65px;overflow:auto;margin-bottom:-200px;}
.sideNav-list {padding:0;}
.sideNav-list li {min-height:50px;border-bottom:1px solid #f0f0f0;position:relative;}
.sideNav-list li a {display:block;width:100%;height:100%;padding:20px 18px 17px;color:#333;font-size:18px;position:relative;}
.sideNav-list > li.on > a {color:#0361ae;font-weight:500;position:relative;}
.sideNav-list > li.on > a:after {content:'';display:inline-block;width:14px;height:14px;background:url('../images/arrow_nav_mob.png') no-repeat;background-size:100%;position:absolute;top:50%;right:18px;margin-top:-7px;}
.sideNav-list li .sub {border-top:1px solid #f0f0f0;background:#fafafa;}
.sideNav-list li .sub li:last-child {border-bottom:none;}
.sideNav-list li .sub li a {padding-left:32px;font-size:17px;color:#666;}
.sideNav-list li .sub li.on a {color:#0361ae;}
.sidebar-util {width:100%;height:65px;border-top:1px solid #e0e0e0;background:#f5f5f5;position:absolute;bottom:0;}
.sidebar-util .sidebar-util-container {margin-top:22px;text-align:center;}
.sidebar-util span {display:inline-block;margin:0 10px;font-size:16px;color:#999;position:relative;}
.sidebar-util span:before {content:'';display:inline-block;width:1px;height:13px;background:#e5e5e5;position:absolute;left:-12px;top:6px;}
.sidebar-util span:first-child:before {content:none;}
.sidebar-util .user-name em {font-weight:500;}
.sidebar-util .btn-logout button {font-size:16px;color:#999;outline:none;vertical-align:top;}
/* left */
.leftNav {position:absolute;top:0;left:-1px;bottom:-1px;width:220px;background:#0269c3;}
.leftNav-list li {min-height:50px;border-bottom:1px solid rgba(255,255,255,0.1);position:relative;}
.leftNav-list li a {display:block;width:100%;height:100%;padding:14px 20px 11px;color:#d6ecff;font-size:16px;}
.leftNav-list li a:hover {color:#fff;}
.leftNav-list > li.on > a {width:230px;color:#fff;font-weight:500;background:#0f377f;}
.leftNav-list li .sub {border-top:1px solid #0b4595;box-shadow:inset 0 4px 0 rgba(0,0,0,0.04);background:#0153ae;}
.leftNav-list li .sub li {border-bottom-color:rgba(255,255,255,0.04);}
.leftNav-list li .sub li:last-child {border-bottom:none;}
.leftNav-list li .sub li a {padding-left:42px;color:#5bafea;font-size:14px;position:relative;}
.leftNav-list li .sub li a:before {content:'';display:inline-block;width:3px;height:3px;background:rgba(168,227,255,0.3);position:absolute;top:24px;left:30px;}
.leftNav-list li .sub li a:hover {color:#fff;}
.leftNav-list li .sub li.on a {color:#fff;}
.leftNav-list li .sub li.on a:before {background:#fff;}
.leftNav-list li .sub li.on a:after {content:'';display:inline-block;width:12px;height:12px;background:url('../images/arrow_nav_subOn.png') no-repeat;position:absolute;top:18px;right:13px;}
@media all and (max-width: 1023px) {
	.leftNav {display:none;}
}
/* footer */
.footer {padding:50px 0;}
.footer .footer-menu {text-align:center;margin-bottom:12px;}
.footer .footer-menu a {display:inline-block;padding:3px 10px 1px;border:1px solid #d6dce2;color:#999;font-size:13px;background:#f2f6fa;}
.footer copyright {display:block;font-size:12px;text-align:center;color:#bbb;letter-spacing:0.5px;position:relative;}
.footer copyright:after {content:'';display:block;width:128px;height:12px;margin:6px auto 0;background:url('../images/footer_logo.png') no-repeat 50%;background-size:100%;}
@media all and (min-width: 1024px) and (max-width: 1099px) {
	.respWidth {padding: 0 20px;}
	.footer {padding:30px 0;}
}
@media all and (max-width: 1023px) {
  .header-pc {display:none;}
  .header-mobile {display:block;position:relative;}
	.header-mobile:after {content:'';display:block;clear:both;}
	.header-mobile h1 {margin:0 auto;font-size:20px;font-weight:500;text-align:center;}
	.header-mobile a.mob-all-menu {display:block;width:22px;height:22px;background:url('../images/gnb_mobile.png') no-repeat center center;background-size:100%;position:absolute;left:10px;top:50%;margin-top:-11px;}
	aside.sidebar-mobile {display:block;}
	main .containerWrap {border:1px solid #d3d3d3;}
}
@media all and  (min-width: 768px) and (max-width: 1023px) {
	.header-mobile {height:80px;}
	.header-mobile h1 {width:252px;}
	.header-mobile h1 a {height:80px;}
	.respWidth {padding: 0 10px;}
	.pageWrap {margin:0 15px 20px 15px;padding-top:25px;}
	.page-title h2 {margin-bottom:20px !important;}
	.footer {padding:30px 0;}
	.sidebar-logo {height:81px;}
	.sidebar-logo h1 {height:81px;}
	.sidebar-nav {padding:81px 0 65px;}
	.sidebar-close {top:28px;}
}
@media all and (max-width: 767px) {
	.respWidth {padding:0 10px;}
	.pageWrap {margin:0 15px 20px 15px;padding-top:20px;}
	.footer {padding:30px 0;}
	.footer .footer-menu a {font-size:12px;}
	.footer copyright {letter-spacing:0.3px;}
	.footer copyright:after {width:107px;height:10px;}
}

/*---------------   Radio & Checkbox & form   ---------------*/
input[type="submit"], input[type="button"], input[type="reset"] {cursor:pointer;-webkit-appearance:none;}
input, select, textarea {font-family:arDotum,'맑은 고딕',Malgun Gothic,'돋움',Dotum,AppleGothic,sans-serif;font-size:15px;color:#404040;-webkit-appearance:none;border-radius:0}
input[type="text"], input[type="search"], input[type="tel"], input[type="number"], input[type="password"], input[type="email"] {-webkit-appearance:none;border-radius:0}
input[placeholder] {font-family:arDotum,'맑은 고딕',Malgun Gothic,'돋움',Dotum,AppleGothic,sans-serif;}
::-webkit-input-placeholder {color: #909090 !important; }
::-moz-placeholder {color: #909090 !important}
:-ms-input-placeholder {color: #909090 !important;}
:-moz-placeholder {color: #909090 !important;}
::-ms-clear, ::-ms-reveal {display: none;}
select::-ms-expand {display:none;}
select {-webkit-appearance:none;-moz-appearance:none;appearance:none;}
textarea {resize:none;padding:10px !important;overflow:auto;}
input:focus, select:focus, textarea:focus {outline:none;}
input[type="radio"], input[type="checkbox"] {display:inline-block;vertical-align:middle;}
input[type='radio'], input[type='checkbox'] {display:none;}
input[type='radio'] + label, input[type='checkbox'] + label {display:inline-block;margin-right:20px;font-weight:400;cursor:pointer;}
input[type='radio'] + label:before {content:'';display:inline-block;width:14px;height:14px;margin:-2px 3px 0 0;vertical-align:middle;overflow:hidden;background:url('../images/form_radio.png') no-repeat left top;background-size:100%;cursor:pointer;}
input[type='radio']:checked + label:before {background-position:left -25px;}
input[type='checkbox'] + label:before  {content:'';display:inline-block;width:14px;height:14px;margin:-2px 3px 0 0;vertical-align:middle;overflow:hidden;background:url('../images/form_radio.png') no-repeat left -50px;background-size:100%/*14px 100px*/;cursor:pointer;}
input[type='checkbox']:checked + label:before {background-position:left -75px;}
input[type='radio'] + label.screen-out, input[type='radio'] + label.screen-out:before, input[type='checkbox'] + label.screen-out, input[type='checkbox'] + label.screen-out:before {margin-right:0;}
label {margin-bottom:0;}
.inputBox {width:100%;height:36px;padding:0 10px;border:1px solid #d0d0d0;vertical-align:top;}
table td .inputBox {vertical-align:middle;}
select.inputBox {height:36px;line-height:1 !important;padding-right:30px;background:#fff url('../images/form_select.png') no-repeat right 50%;background-size:30px 40px;}
select.inputBox[disabled] {background:#eee url('../images/form_select_dis.png') no-repeat right 50%;background-size:30px 40px;}
textarea.inputBox {width:100%;padding:5px;padding:0 10px;border:1px solid #d0d0d0;}
@media all and (max-width: 767px) {
	input, select, textarea {font-size:13px;padding:0 5px;}
	.inputBox	{padding:0 5px;}
	input[type='radio'] + label:before, input[type='checkbox'] + label:before {width:20px;height:20px;background:url('../images/form_radio_mobile.png') no-repeat left top;background-size:100%;}
	input[type='radio']:checked + label:before {background-position:left -25px;}
	input[type='checkbox'] + label:before {background-position:left -50px;}
	input[type='checkbox']:checked + label:before {background-position:left -75px;}
}
/*datepicker*/
.inputBoxDate {display:inline-block;min-width:102px;height:36px;border:1px solid #d0d0d0;vertical-align:middle;position:relative;}
.inputBoxDate:after {content:'';display:block;clear:both;}
.inputBoxDate input.inputBox {float:left;width:70%;width:calc(100% - 30px);height:34px;padding-right:0;border:none;}
.inputBoxDate .btn-date {width:30px;height:34px;background:#fff;position:absolute;right:0;top:0;padding:0;}
.inputBoxDate .btn-date .ico-date {position:relative;}
.inputBoxDate .btn-date .ico-date:before {content:'';display:inline-block;width:18px;height:18px;margin-top:2px;background:url('../images/ico_calendar.png') no-repeat 50% 50%;background-size:100%;}
.divForm li .inputBoxDate {display:block;}
.inputBoxDate input.inputBox.datepicker {width:100%;padding-right:30px;background:#fff url('../images/ico_calendar.png') no-repeat 94% 50%;background-size:18px;}

/*---------------   Buttons   ---------------*/
.btn-wrap {margin:20px 0;}
.btnC {text-align:center;}
.btnR {text-align:right;}
button {font-family:arDotum,'맑은 고딕',Malgun Gothic,'돋움',Dotum,AppleGothic,sans-serif;}
button {background:transparent;vertical-align:middle;margin:0;-webkit-appearance:none;border-radius:0;cursor:pointer;}
button.btn {display:inline-block;text-align:center;font-size:15px;font-weight:500;letter-spacing:0px;line-height:1;border-radius:0;
	-webkit-transition: all .2s ease-out;
     -moz-transition: all .2s ease-out;
        -o-transition: all .2s ease-out;
	          transition: all .2s ease-out;
}
.btn:focus, .btn:active {box-shadow:none;outline-offset:none;}
.btn-red {border:1px solid #0259ba;color:#fff;background:#0259ba;}
.btn-red:hover, .btn-red:active, .btn-red:focus {border:1px solid #054ca2;color:#fff;background:#054ca2;}
.btn-white {border:1px solid #a0a0a0;background:#fff;color:#505050;}
.btn-gray {border:1px solid #84858d;background:#84858d;color:#fff;}
.btn-gray:hover, .btn-gray:active, .btn-gray:focus {border:1px solid #74767e;color:#fff;background:#74767e;}
.btn-gold {border:1px solid #84858d;background:#84858d;color:#fff;}
.btn-blue {border:1px solid #84858d;background:#84858d;color:#fff;}
.btn-gray:hover, .btn-gray:focus, .btn-gold:hover, .btn-gold:visited, .btn-gold:focus, .btn-blue:hover, .btn-blue:visited, .btn-blue:focus {color:#fff !important;}
.btn-red-point {border:1px solid #e12b64;color:#fff;background:#e12b64;}
.btn-red-point:hover, .btn-red-point:active, .btn-red-point:focus {border:1px solid #d61954;color:#fff;background:#d61954;}
.btn.btn-prim {height:36px;padding:0 30px;}
.btn.btn-second {height:30px;padding:0 20px;border-radius:2px;}
.btn.btn-3rd {height:31px;padding:0 10px;border-radius:2px;font-size:14px;}
.btn.btn-4th {height:28px;padding:0 8px;border-radius:2px;font-size:14px;}
.btn.btn-5th {height:36px;padding:0 8px;border-radius:2px;font-size:14px;}
.btn.btn-dis {border:none;box-shadow:inset 1px 1px 0 rgba(0,0,0,0.1);background:#c6c7cb;cursor:default;outline:none;}
.btn.btn-dis:focus, .btn.btn-dis:active {outline:none;}
.btn.btn-narr {padding-left:10px;padding-right:10px;}
.btn-icon {width:20px;height:20px;padding:0;border-radius:3px !important;text-indent:100%;white-space:nowrap;overflow:hidden;}
.btn-icon .ico {position:relative;}
.btn-icon .ico:before {content:'';display:block;width:20px;height:20px;background:url('../images/ico_add.png') no-repeat;background-size:100%;}
.btn-icon .ico.minus:before {background-position:-1px -1px;}
.btn-icon .ico.plus:before {background-position:-1px -21px;}
.ico-down {display:inline-block;position:relative;}
.ico-down:before {content:'';display:block;width:14px;height:14px;margin:2px 5px -2px -5px;background:url('../images/ico_download.png') no-repeat;background-size:100%;}
.btn.btn-5th.btn-blue.btn_file {height:36px;line-height:34px;padding:0 8px;border-radius:3px;font-size:14px;color:#666;border:1px solid #c0c0c0;background:#fff;}
.btn.btn-5th.btn-blue.btn_file:hover {color:#666 !important;}
@media all and (max-width: 767px) {
	.btn.btn-prim {height:40px;}
	.btn.wd50 {width:49%;}
	.btn-wrap	.btn.wd100 {margin-bottom:5px;}
	.btn-wrap	.btn.wd100:last-child {margin-bottom:0;}
	.btn-wrap	.btn.wd100:only-child {margin:0;}
	.btn.wd100 {margin-bottom:5px;}
	.btn.wd100:last-child, .btn.wd100:only-child  {margin-bottom:0;}
}

/*---------------   Title   ---------------*/
.page-title {text-align:right;}
.page-title:after {content:'';display:block;clear:both;}
.page-title h2 {float:left;color:#202020;font-size:26px;font-weight:500;line-height:1;margin-bottom:30px;}
.page-title .page-location {display:inline-block;margin-top:7px;line-height:1;color:#b0b0b0;font-size:13px;}
.page-title .page-location .home {display:inline-block;}
.page-title .page-location b {font-weight:400;color:#888;}
.page-title-sub {font-size:17px;font-weight:500;line-height:1;margin:-5px 0 10px;clear:both;}
.page-title-sub h3 {font-weight:500;color:#303030;}
.page-title-sub.grid-filter h3 {float:left;margin-top:7px;}
.page-desc-box {margin-bottom:10px;padding:0 10px;}
.page-desc-box p {padding-left:8px;position:relative;}
.page-desc-box p:before {content:'';display:inline-block;width:3px;height:3px;border-radius:1px;background:#b0b0b0;position:absolute;left:0;top:8px;}
.page-content.respMobile {display:none;}
.respMobile {border-top:1px solid #eee;}
.respMobile .respMobile-alert {margin:100px 0 100px;}
.respMobile .respMobile-alert span {display:block;margin:0 auto;padding:0 30px;text-align:center;font-size:20px;font-weight:500;color:#909090;position:relative;}
.respMobile .respMobile-alert span:before {content:'';display:block;width:60px;height:60px;margin:0 auto 20px;background:url('../images/img_nomobile.png') no-repeat;background-size:100%;}
@media all and (max-width: 767px) {
	.page-title h2 {font-size:20px;margin-bottom:20px;}
	.page-title .page-location {display:none;}
	.page-title-sub {font-size:16px;}
	.page-content.respMobile {display:block;}
	.page-content.respPC {display:none;}
}

/*---------------   Table   ---------------*/
.list-total {color:#202020;}
.list-total .txtRed {font-weight:500;}
/* 목록 */
.tbl-list {table-layout:fixed;}
.tbl-list th, .tbl-list td {min-height:42px;height:42px;line-height:1.2;padding:5px 5px 1px;text-align:center;word-break:break-all;}
.tbl-list th {border-bottom:1px solid #e0e0e0;border-left:1px solid #eee;background-color:#f9f9f9;}
.tbl-list td {border-left:1px solid #eee;border-top:1px solid #eee;}
.tbl-list tr th:first-child, .tbl-list tr td:first-child {border-left:none;}
.tbl-list .tbl-scroll tr:last-of-type td {border-bottom:1px solid #eee;}
.tbl-list tr.panel-black td {background:#f5f5f5;}
.tbl-scroll {overflow:auto;}
.tbl-scroll.tbl-ht10 {height:463px;}
.tbl-list td .divForm {margin:-2px 0 2px;}
/* 쓰기, 수정 테이블 */
.tbl-wm {border-top:2px solid #777;border-bottom:1px solid #d0d0d0;}
.tbl-wm th, .tbl-wm td {min-height:42px;height:42px;line-height:1.2;border-left:1px solid #eee;border-top:1px solid #eee;}
.tbl-wm th {padding:4px 20px 2px;text-align:left;background-color:#f9f9f9;}
.tbl-wm td {padding:3px 10px;background-color:#fff;}
.tbl-wm tr:first-child th, .tbl-wm tr:first-child td {border-top:none;}
.tbl-wm tr th:first-child {border-left:none;}
.tbl-wm .tbl-scroll tr:last-of-type th, .tbl-wm .tbl-scroll tr:last-of-type td {border-bottom:1px solid #eee;}
.tbl-wm td[colspan] {border-left:none;}
@media all and (max-width: 767px) {
	.tbl-wm th, .tbl-wm td {padding:4px 10px 2px;}
}
@media all and (max-height:768px) {
	.tbl-scroll.tbl-ht10.tbl-mob {height:253px;}
}
