<script>
	// 탭 : 페이지
	$(function() {
		$('ul.tab li').click(function() {
			var activeTab = $(this).attr('data-tab');
			$('ul.tab li').removeClass('current');
			$('.tabcontent').removeClass('current');
			$(this).addClass('current');
			$('#' + activeTab).addClass('current');
		})
	});
	
	// 탭 : 팝업
	$(function() {
		$('ul.tab-popup li').click(function() {
			var activeTab = $(this).attr('data-tab');
			$(this).parents(".popup-content").find('ul.tab-popup li').removeClass('current');
			$(this).parents(".popup-content").find('.tabcontent-popup').removeClass('current');
			$(this).addClass('current');
			$(this).parents(".popup-content").find('#' + activeTab).addClass('current');
		})
	});


	// QnA
	function qna(idMyDiv){
		 var objDiv = document.getElementById(idMyDiv);
		 if(objDiv.style.display==""){ objDiv.style.display = "none"; }
		 else{ objDiv.style.display = ""; }
	}

