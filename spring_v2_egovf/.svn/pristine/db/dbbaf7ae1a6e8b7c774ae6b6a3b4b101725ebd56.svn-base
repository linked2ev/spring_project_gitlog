@CHARSET "UTF-8";

/*--------------- 추가 CSS ---------------*/
/* 참가신청 > 상세설문 약관동의 */
/* .rndBaseSurvey2, .rndAddInfo, .rndBaseSurvey {margin-left:39px;} */ 
.rndBaseSurvey2 .term-title {border:1px solid #e0e0e0;border-bottom:none;}
.rndBaseSurvey2 .term-title span {display:inline-block;padding:10px 20px 8px;font-size:16px;font-weight:500;color:#202020;}
.rndBaseSurvey2 .term-wrap {height:180px;border:1px solid #e0e0e0;border-top-color:#eee;overflow-y:auto;}
.rndBaseSurvey2 .term-wrap .term-text {padding:30px;line-height:1.4;}
.rndBaseSurvey2 .termAgree-chk {position:relative;padding:16px 25px 14px;border:1px solid #e0e0e0;border-top:none;background-color:#f9f9f9;}
input[type='checkbox'].chk-agree {display:none;}
input[type='checkbox'].chk-agree + label {display:inline-block;margin-right:16px;cursor:pointer;}
input[type='checkbox'].chk-agree + label:before {content:'';display:inline-block;width:18px;height:18px;margin:-2px 6px 0 0;vertical-align:middle;overflow:hidden;background:url('../images/form_radio_term.png') no-repeat left top;background-size:18px 50px;cursor:pointer;}
input[type='checkbox'].chk-agree:checked + label:before {background-position:left -25px;} 
@media all and (max-width: 767px) {
	.rndTerm .term-title span {padding:10px 20px 8px;font-size:15px;}
	.rndTerm .term-wrap {height:140px;}
	.rndTerm .term-wrap .term-text {padding:20px}
	.rndTerm .termAgree-chk {padding:14px 15px;}
}

