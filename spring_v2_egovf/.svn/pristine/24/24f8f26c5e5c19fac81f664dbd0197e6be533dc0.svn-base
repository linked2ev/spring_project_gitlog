<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="Cmmn">
	
    
	<!-- 공통 목록 -->
    <select id="selectCmmnList" parameterClass="HashMap" resultClass="HashMap">
        /* Cmmn.selectCmmnList */
		SELECT
				COMM_CD
				,CODE
				,VALUE
		FROM 
				RDCM102DT
		WHERE 
		 		COMM_CD = #COMM_CD#
    </select>

    
    <!-- 첨부파일 삭제 -->
    <delete id="deleteAttachFileDelete" parameterClass="HashMap">
        /* Cmmn.deleteAttachFileDelete */
        DELETE FROM 
        	RDEI105DT 
        WHERE
            USER_ID = #SESSION_USER_ID#
            AND A.EXAM_MNG_NO = #EXAM_MNG_NO#
    </delete>
    
    
</sqlMap>
