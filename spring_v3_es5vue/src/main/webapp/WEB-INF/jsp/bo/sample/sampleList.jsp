<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%><jsp:include page="/WEB-INF/jsp/bo/include/head.jsp" /></head><body><!-- header --><jsp:include page="/WEB-INF/jsp/bo/include/header/header.jsp" /><!-- // header --><!-- main --><main id="main" class="main respWidth">	<div class="containerWrap">	 	<!-- leftNav -->  	<jsp:include page="/WEB-INF/jsp/bo/include/left/left.jsp" />	<!-- // leftNav --> 		 		<div class="pageWrap rndRecrNotice"> 			<div class="page-title">				<h2>참여 모집 공고</h2>				<div class="page-location">					<span class="home">홈</span> - <b>참여 모집 공고</b>				</div>			</div>			 			<div class="page-content">				<div class="searchWrap">					<form id="searchForm" name="searchForm">					<input type="hidden" name="cPageNo" id="cPageNo" value="<c:out value="${commandMap.cPageNo }"/>"/>					<input type="hidden" name="EXAM_MNG_NO" value="<c:out value="${commandMap.EXAM_MNG_NO }"/>"/>					<input type="hidden" name="EXAM_WAY_CD" value="<c:out value="${commandMap.EXAM_WAY_CD }"/>"/>										<div class="search-cont">						<div class="search-l">							<dl class="sch-multi">								<dt>연구 구분</dt>								<dd>									<select id="searchTeamGbn" name="searchTeamGbn" title="연구 구분" class="inputBox">	                                    <option value="" <c:if test="${empty commandMap.searchTeamGbn }">selected="selected"</c:if>>전체</option>	                                    <c:if test="${not empty SURVEY_GBN_LIST }">	                                        <c:forEach var="codeRow" items="${SURVEY_GBN_LIST }" varStatus="i">	                                            <option value="<c:out value="${codeRow.CODE }"/>" <c:if test="${codeRow.CODE eq commandMap.searchTeamGbn }">selected="selected"</c:if>>	                                                <c:out value="${codeRow.CODE }"/>	                                            </option>	                                        </c:forEach>	                                    </c:if>	                                </select>								</dd>								<dt>연구 방식</dt>								<dd>									<ul class="divForm wd50">										<li>											<select id="searchExamWay" name="searchExamWay" title="연구 방식" class="inputBox">												<option value="" <c:if test="${empty commandMap.searchExamWay }">selected="selected"</c:if>>전체</option>			                                    <c:if test="${not empty TEST_PLAN_LIST }">			                                        <c:forEach var="codeRow" items="${TEST_PLAN_LIST }" varStatus="i">			                                            <option value="<c:out value="${codeRow.CODE }"/>" <c:if test="${codeRow.CODE eq commandMap.searchExamWay }">selected="selected"</c:if>>			                                                <c:out value="${codeRow.VALUE }"/>			                                            </option>			                                        </c:forEach>	                                    		</c:if>	                                		</select>										</li>										<li>											<select id="searchVisitArea" name="searchVisitArea" title="방문위치" class="inputBox">												<option value="" <c:if test="${empty commandMap.searchVisitArea }">selected="selected"</c:if>>전체</option>			                                    <c:if test="${not empty VISIT_AREA_LIST }">			                                        <c:forEach var="codeRow" items="${VISIT_AREA_LIST }" varStatus="i">			                                            <option value="<c:out value="${codeRow.CODE }"/>" <c:if test="${codeRow.CODE eq commandMap.searchVisitArea }">selected="selected"</c:if>>			                                                <c:out value="${codeRow.VALUE }"/>			                                            </option>			                                        </c:forEach>			                                    </c:if>			                                </select>																</li>									</ul>											</dd>							</dl>							<dl class="sch-multi single">								<dt>연구 제목</dt>								<dd><input type="text" id="searchExamTitle" name="searchExamTitle" placeholder="연구 제목을 입력하세요" class="inputBox" value="<c:out value="${commandMap.searchExamTitle }"/>"></dd>							</dl>						</div>						<div class="search-r">							<div class="search-r-cont">								<button class="btn btn-search" id="searchPc"><i class="ico-search"></i>조회</button>							</div>						</div>						<div class="search-r-mobile">							<div class="search-r-cont">								<button class="btn btn-search" id="searchMb"><i class="ico-search"></i>조회</button>							</div>						</div>					</div>					</form><!-- //searchForm -->				</div><!-- //searchWrap -->				<div class="listWrap">					<ul class="recruit-list recruit-now">						<c:choose>							<c:when test="${not empty list }">								<c:forEach var="row" items="${list}" varStatus="i">									<li>										<div class="recruit-content recruit-current">											<div class="recruit-l">												<div class="recruit-r">													<div class="recruit-label">													    <c:if test="${row.STATUS eq 'selection'}"><c:set var="STATUS" value="green" /></c:if>													    <c:if test="${row.STATUS eq 'applying'}"><c:set var="STATUS" value="red" /></c:if>													    <c:if test="${row.STATUS eq 'available'}"><c:set var="STATUS" value="blue" /></c:if>														<span class="label-<c:out value="${STATUS}"/> "> 															<c:if test="${row.STATUS eq 'selection'}">패널선정</c:if>															<c:if test="${row.STATUS eq 'applying'}">신청중</c:if>															<c:if test="${row.STATUS eq 'available'}">신청가능</c:if>														</span>													</div>												</div>												<div class="recuit-title">													<a href="#" class="fn_goView" data-exam_mng_no="${row.EXAM_MNG_NO }" data-exam_way_cd="${row.EXAM_WAY_CD }">														<em><c:out value="${row.TEAM_GBN }"/></em>														<c:out value="${row.EXAM_TITLE }"/>  													</a>												</div>												<div class="recruit-period">													<span class="recruit-date">신청기간 : <c:out value="${row.APP_PERIOD }"/></span>													<span class="exam-date">시험기간 : <c:out value="${row.EXAM_PERIOD }"/></span>												</div>												<div class="recruit-tag">													<span><c:out value="${row.EXAM_WAY }"/></span>													<span><c:out value="${row.RECRUIT_WAY }"/></span>												</div>											</div>										</div>									</li>								</c:forEach>							</c:when>							<c:otherwise>								<div class="tbl-nodata"><div class="no-data"><span>참여 모집 공고가 없습니다.</span></div></div>							</c:otherwise>						</c:choose>					</ul>				</div>				  				<div class="pageNavi" id="pageNavi">        			<ui:paginationInfo paginationInfo="${paginationInfo}" jsFunction="fn_goPage" />        		</div>        				</div><!-- // page-content -->		</div><!-- // pageWrap -->	</div><!-- // container --></main><!-- // main --><!-- footer --><jsp:include page="/WEB-INF/jsp/bo/include/footer.jsp" /><!-- // footer --><script>$(function(){       	// 연구 방식 변경	var searchExamWay = "<c:out value='${commandMap.searchExamWay }'/>";	if(searchExamWay != "01"){		$("#searchVisitArea").hide();		$("#searchVisitArea").val("");	}    // 연구 방식 변경 이벤트    $(document).on("change", "select[name='searchExamWay']", function () {    	if($("select[name='searchExamWay']").val() == "01"){    		$("#searchVisitArea").show();    		$("#searchVisitArea").val("");    	}else{    		$("#searchVisitArea").hide();    		$("#searchVisitArea").val("");    	}    });         // 조회 버튼 클릭 이벤트    $(document).on("click", ".btn-search", function () {        var $searchForm = $("#searchForm");        $("#cPageNo").val(1);        $searchForm.attr({"action":"<c:url value='/bo/sample/sampleList.do'/>", "target":"_self", "method":"post"}).submit();    });	    // 시험모집공고 상세보기 이벤트    $(document).on("click", ".fn_goView", function () {    	var $searchForm = $("#searchForm");     	var examMngNo = $(this).data("exam_mng_no");    	var examWayCd = $(this).data("exam_way_cd");    	    	$("input[name='EXAM_MNG_NO']").val(examMngNo);    	$("input[name='EXAM_WAY_CD']").val(examWayCd);        $searchForm.attr({"action":"<c:url value='/bo/sample/sampleView.do'/>", "target":"_self", "method":"post"}).submit();    });     });// 페이징 조회 이벤트function fn_goPage(cPageNo) {	var $searchForm = $("#searchForm");	    $("input[name='cPageNo']").val(cPageNo);    $searchForm.attr({"action":"<c:url value='/bo/sample/sampleList.do'/>", "target":"_self", "method":"post"}).submit();}</script></body></html>